lse {\r\n        this.$router.push({\r\n          path: `/denglu`\r\n        })\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    getMyUserInfo().then(res => {\r\n      let data = res.data.data[0]\r\n      overUserInfo(data)\r\n      if (isLogin()) {\r\n        this.myLogin = getUserInfo().Account\r\n        this.myRegis = '退出'\r\n      } else {\r\n        this.myLogin = '登录'\r\n        this.myRegis = '注册'\r\n      }\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#mylogin {\r\n  display: flex;\r\n  /* width: 100px; */\r\n  height: 40px;\r\n  background-color: rgb(177, 84, 79);\r\n  line-height: 40px;\r\n}\r\na {\r\n  display: inline-block;\r\n  padding: 0 10px;\r\n  color: white;\r\n  text-decoration: none;\r\n  font-size: 12px;\r\n  min-width: 30px;\r\n}\r\nspan {\r\n  display: inline-block;\r\n  color: white;\r\n  margin-left: 5px;\r\n}\r\n.zhuce {\r\n  display: inline-block;\r\n  padding: 0 10px;\r\n  width: 30px;\r\n}\r\na:hover {\r\n  color: rgba(255, 255, 255, 0.5);\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/home/components"}]}