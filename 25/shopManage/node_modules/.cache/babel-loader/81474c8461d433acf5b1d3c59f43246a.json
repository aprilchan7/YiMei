).then(res => {\n      let data = res.data.data[0]\n      overUserInfo(data)\n      if (isLogin()) {\n        this.myLogin = getUserInfo().Account\n        this.myRegis = '退出'\n      } else {\n        this.myLogin = '登录'\n        this.myRegis = '注册'\n      }\n    })\n  }\n}\n</script>\n\n<style scoped>\n#mylogin {\n  width: 100px;\n  height: 40px;\n  background-color: rgb(177, 84, 79);\n  line-height: 40px;\n}\na {\n  color: white;\n  text-decoration: none;\n  font-size: 12px;\n}\nspan {\n  color: white;\n  margin-left: 5px;\n}\n.zhuce {\n  margin-left: 5px;\n}\na:hover {\n  color: rgba(255, 255, 255, 0.5);\n}\n</style>\n"],"sourceRoot":"src/views/home/components"}]}